<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="ParserRunnerPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="340.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="crsxviz.application.crsxrunner.Controller">
   <children>
      <VBox layoutY="7.0" prefHeight="25.0" prefWidth="600.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
         <children>
            <HBox prefHeight="0.0" prefWidth="135.0">
               <children>
                  <Label minWidth="-Infinity" prefWidth="175.0" text="CRSX compiled program to run: " HBox.hgrow="SOMETIMES">
                     <HBox.margin>
                        <Insets left="10.0" />
                     </HBox.margin>
                     <padding>
                        <Insets top="5.0" />
                     </padding>
                  </Label>
                  <TextField fx:id="txtExecutable" prefHeight="25.0" prefWidth="338.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="btnBrowseExe" minWidth="-Infinity" mnemonicParsing="false" onKeyReleased="#onExeBrowseKey" onMouseClicked="#onClickExeBrowse" prefWidth="70.0" text="Browse">
                     <padding>
                        <Insets bottom="3.0" left="5.0" right="5.0" top="3.0" />
                     </padding>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label minWidth="-Infinity" prefHeight="17.0" prefWidth="60.0" text="Wrapper: " HBox.hgrow="SOMETIMES">
                     <HBox.margin>
                        <Insets left="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtWrapper" prefHeight="25.0" prefWidth="520.0" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </TextField>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
            <HBox minHeight="-Infinity" prefHeight="125.0">
               <children>
                  <Label minWidth="-Infinity" prefWidth="35.0" text="Term: " HBox.hgrow="SOMETIMES">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <Accordion fx:id="acrdnTermPanel" HBox.hgrow="ALWAYS">
                    <panes>
                      <TitledPane fx:id="pnlFileBrowse" animated="false" text="Import from file">
                           <content>
                              <Pane prefHeight="200.0" prefWidth="200.0">
                                 <children>
                                    <HBox fillHeight="false" prefHeight="25.0" prefWidth="535.0">
                                       <children>
                                          <Label text="Path: ">
                                             <HBox.margin>
                                                <Insets left="10.0" top="5.0" />
                                             </HBox.margin>
                                          </Label>
                                          <TextField fx:id="txtInFilePath" HBox.hgrow="ALWAYS">
                                             <HBox.margin>
                                                <Insets left="10.0" right="10.0" />
                                             </HBox.margin>
                                          </TextField>
                                          <Button fx:id="btnBrowseInFile" minWidth="-Infinity" mnemonicParsing="false" onKeyReleased="#onBrowseInFileKey" onMouseClicked="#onInFileBrowseClicked" prefWidth="60.0" text="Browse">
                                             <HBox.margin>
                                                <Insets right="10.0" />
                                             </HBox.margin>
                                          </Button>
                                       </children>
                                       <padding>
                                          <Insets top="25.0" />
                                       </padding>
                                    </HBox>
                                 </children>
                              </Pane>
                           </content>
                        </TitledPane>
                      <TitledPane fx:id="pnlInputText" animated="false" prefHeight="64.0" prefWidth="504.0" text="Input as text">
                           <content>
                              <Pane prefHeight="200.0" prefWidth="200.0">
                                 <children>
                                    <TextArea fx:id="txtaInTerm" prefHeight="75.0" prefWidth="537.0" />
                                 </children>
                              </Pane>
                           </content>
                        </TitledPane>
                    </panes>
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Accordion>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Label minWidth="-Infinity" prefWidth="120.0" text="Output trace file path: ">
                     <HBox.margin>
                        <Insets left="10.0" top="5.0" />
                     </HBox.margin>
                  </Label>
                  <TextField fx:id="txtOutPath" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets left="10.0" right="10.0" />
                     </HBox.margin>
                  </TextField>
                  <Button fx:id="btnBrowseOutPath" minWidth="-Infinity" mnemonicParsing="false" onKeyReleased="#onOutFileBrowseKey" onMouseClicked="#onOutFileBrowseClicked" prefWidth="60.0" text="Browse">
                     <HBox.margin>
                        <Insets right="10.0" />
                     </HBox.margin>
                  </Button>
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
            <HBox prefHeight="100.0" prefWidth="200.0">
               <children>
                  <Button fx:id="btnStart" mnemonicParsing="false" onKeyPressed="#onStartKey" onMouseClicked="#onStartClicked" text="Start Processing">
                     <HBox.margin>
                        <Insets left="10.0" top="15.0" />
                     </HBox.margin>
                  </Button>
                  <Label text="Status: ">
                     <HBox.margin>
                        <Insets left="10.0" right="3.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <Label fx:id="lblStatus" prefHeight="17.0" prefWidth="347.0" text="Not Started" HBox.hgrow="ALWAYS">
                     <HBox.margin>
                        <Insets right="10.0" top="20.0" />
                     </HBox.margin>
                  </Label>
                  <ProgressIndicator fx:id="statusIndicator" minHeight="50.0" visible="false" HBox.hgrow="SOMETIMES" />
               </children>
               <VBox.margin>
                  <Insets top="15.0" />
               </VBox.margin>
            </HBox>
         </children>
      </VBox>
   </children>
   <cursor>
      <Cursor fx:constant="DEFAULT" />
   </cursor>
</AnchorPane>